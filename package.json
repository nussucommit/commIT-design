{
  "name": "commit-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.16",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "auto": "^10.29.3",
    "cross-env": "^7.0.3",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-select": "^4.3.1",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "chromatic": "npx chromatic"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-scroll": "^1.8.3",
    "@types/react-select": "^4.0.17",
    "chromatic": "^5.9.2"
  },
  "description": "A React components library made by NUSSU commIT's 30th Technical MC",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nussucommit/commIT-ui.git"
  },
  "keywords": [
    "react"
  ],
  "author": "NUSSU-commIT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nussucommit/commIT-ui/issues"
  },
  "homepage": "https://github.com/nussucommit/commIT-ui#readme"
}
